<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Magento_DataExporterStatus::css/export-status.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block name="data_exporter_feed_listing_container" class="Magento\Backend\Block\Template"
                   template="Magento_DataExporterStatus::section.phtml">
                <block name="data_exporter_feed_status_info_block"
                       class="Magento\DataExporterStatus\Block\Adminhtml\Section"
                       template="Magento_DataExporterStatus::feeds_listing_columns_info.phtml">
                    <arguments>
                        <argument name="title" xsi:type="string">Column Descriptions</argument>
                        <argument name="sectionOpen" xsi:type="boolean">false</argument>
                    </arguments>
                </block>
            </block>
            <uiComponent name="data_exporter_feeds_listing_component"/>
        </referenceContainer>
    </body>
</page>
